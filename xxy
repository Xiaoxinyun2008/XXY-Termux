# --- [SECTION 0] å¥‡ç‚¹åˆå§‹åŒ– ---
tput cnorm
clear
echo -e "\033[1;35m[INIT] æ­£åœ¨è§£å‹ v4000.0 æ ¸å¿ƒç»„ä»¶...\033[0m"

# 0.1 æ·±åº¦æ¸…ç†ä¸é‡ç½®
for proc in sshd termux-x11 pulseaudio Xwayland vncserver; do
    if pgrep -x "$proc" >/dev/null; then killall -9 "$proc" >/dev/null 2>&1; fi
done
rm -f "$PREFIX/bin/xxy"
rm -rf "$PREFIX/tmp/xxy_core"
mkdir -p "$PREFIX/tmp/xxy_core"

# 0.2 å†™å…¥ä¸»ç¨‹åº
cat > $PREFIX/bin/xxy << 'MAIN_EOF'
#!/data/data/com.termux/files/usr/bin/bash

# ==============================================================================
# ğŸ—ï¸ å…¨å±€é…ç½® (Global Config)
# ==============================================================================
export APP_VER="v4000.0 å¥‡ç‚¹"
export PREFIX="/data/data/com.termux/files/usr"
export TMP_DIR="$PREFIX/tmp/xxy_core"
export ROOTFS="$PREFIX/var/lib/proot-distro/installed-rootfs"
export LOG_FILE="$TMP_DIR/runtime.log"
export THEME_CONF="$TMP_DIR/theme.conf"
export LANG="C.UTF-8"

# å¼‚å¸¸æ•è·
trap 'tput cnorm; stty echo; echo -e "\n\033[0mğŸ›‘ ç³»ç»Ÿå·²æŒ‚èµ·ã€‚"; exit 0' EXIT INT TERM

# ==============================================================================
# ğŸ¨ ä¸»é¢˜å¼•æ“ (Theme Engine)
# ==============================================================================
# é»˜è®¤ä¸»é¢˜: èµ›åšæœ‹å…‹
set_theme_cyber() {
    C_FRAME=$(tput setaf 57)  # è¾¹æ¡†ç´«
    C_TITLE=$(tput setaf 51)  # æ ‡é¢˜é’
    C_TEXT=$(tput setaf 255)  # æ–‡æœ¬ç™½
    C_HL=$(tput setaf 226)    # é«˜äº®é»„
    C_OK=$(tput setaf 46)     # æˆåŠŸç»¿
    C_ERR=$(tput setaf 196)   # é”™è¯¯çº¢
    C_DIM=$(tput setaf 240)   # æš—ç°
    THEME_NAME="Cyberpunk"
}

set_theme_hacker() {
    C_FRAME=$(tput setaf 22)  # æ·±ç»¿
    C_TITLE=$(tput setaf 46)  # äº®ç»¿
    C_TEXT=$(tput setaf 194)  # æ·¡ç»¿
    C_HL=$(tput setaf 154)    # è§å…‰ç»¿
    C_OK=$(tput setaf 46)
    C_ERR=$(tput setaf 196)
    C_DIM=$(tput setaf 236)
    THEME_NAME="Matrix"
}

set_theme_ocean() {
    C_FRAME=$(tput setaf 17)  # æ·±è“
    C_TITLE=$(tput setaf 39)  # äº®è“
    C_TEXT=$(tput setaf 255)
    C_HL=$(tput setaf 45)     # æ·¡è“
    C_OK=$(tput setaf 46)
    C_ERR=$(tput setaf 196)
    C_DIM=$(tput setaf 240)
    THEME_NAME="DeepSea"
}

# åˆå§‹åŒ–é»˜è®¤ä¸»é¢˜
set_theme_cyber
C_RESET=$(tput sgr0)

# ==============================================================================
# ğŸ–¥ï¸ æ¸²æŸ“å†…æ ¸ (Render Kernel) - å·®å¼‚åŒ–æ¸²æŸ“æŠ€æœ¯
# ==============================================================================

# [UI-1] ç»å¯¹å®šä½ç»˜åˆ¶
draw_at() {
    tput cup $1 $2
    echo -ne "$3"
}

# [UI-2] ç»˜åˆ¶é™æ€æ¡†æ¶ (åªæ‰§è¡Œä¸€æ¬¡)
draw_frame_static() {
    clear
    # ç»˜åˆ¶å¤–æ¡† (Box Drawing)
    # å‡è®¾æ ‡å‡†å®½åº¦ 46
    local H="â”"; local V="â”ƒ"
    
    # Top
    draw_at 0 0 "${C_FRAME}â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”“${C_RESET}"
    # Title Area
    draw_at 1 0 "${C_FRAME}â”ƒ${C_RESET}"
    draw_at 1 47 "${C_FRAME}â”ƒ${C_RESET}"
    # Separator
    draw_at 2 0 "${C_FRAME}â”£â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”«${C_RESET}"
    
    # Status Area (Lines 3-6)
    for i in {3..6}; do
        draw_at $i 0 "${C_FRAME}â”ƒ${C_RESET}"
        draw_at $i 47 "${C_FRAME}â”ƒ${C_RESET}"
    done
    
    # Separator
    draw_at 7 0 "${C_FRAME}â”£â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”«${C_RESET}"
    
    # Menu Area (Lines 8-17)
    draw_at 8  2 "${C_TITLE}[1]${C_TEXT} è¿›å…¥ç³»ç»Ÿ (Login)${C_RESET}"
    draw_at 8  26 "${C_TITLE}[5]${C_TEXT} å¼€å‘è€…å †æ ˆ${C_RESET}"
    
    draw_at 9  2 "${C_TITLE}[2]${C_TEXT} å®‰è£…ç³»ç»Ÿ (Install)${C_RESET}"
    draw_at 9  26 "${C_TITLE}[6]${C_TEXT} ç³»ç»ŸåŒ»ç”Ÿ${C_RESET}"
    
    draw_at 10 2 "${C_TITLE}[3]${C_TEXT} å¯åŠ¨æ¡Œé¢ (GUI)${C_RESET}"
    draw_at 10 26 "${C_TITLE}[7]${C_TEXT} ä¸»é¢˜åˆ‡æ¢${C_RESET}"
    
    draw_at 11 2 "${C_TITLE}[4]${C_TEXT} é¹°çœ¼é›·è¾¾ (SSH)${C_RESET}"
    draw_at 11 26 "${C_TITLE}[8]${C_TEXT} å¤‡ä»½è¿˜åŸ${C_RESET}"
    
    draw_at 12 2 "${C_HL}[9]${C_TEXT} å°ç™½è¯´æ˜ä¹¦${C_RESET}"
    draw_at 12 26 "${C_ERR}[0]${C_TEXT} é€€å‡ºç¨‹åº${C_RESET}"
    
    # Fill Borders for Menu
    for i in {8..13}; do
        draw_at $i 0 "${C_FRAME}â”ƒ${C_RESET}"
        draw_at $i 47 "${C_FRAME}â”ƒ${C_RESET}"
    done
    
    # Bottom
    draw_at 13 0 "${C_FRAME}â”—â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”›${C_RESET}"
    
    draw_at 15 1 "${C_HL}â¤ æŒ‡ä»¤:${C_RESET} "
}

# [UI-3] åŠ¨æ€æ•°æ®åˆ·æ–° (å·®å¼‚åŒ–æ›´æ–°)
refresh_hud() {
    # è·å–æ•°æ®
    local time=$(date '+%H:%M:%S')
    local ip=$(ifconfig 2>/dev/null | grep -oE 'inet (addr:)?([0-9]*\.){3}[0-9]*' | grep -v '127.0.0.1' | head -n 1 | awk '{print $2}' | sed 's/addr://'); [ -z "$ip" ] && ip="ç¦»çº¿"
    local ram=$(free -m | awk 'NR==2{printf "%s/%s MB", $3, $2}')
    local disk=$(df -h "$PREFIX" | awk 'NR==2{print $4}')
    
    # SSH çŠ¶æ€
    local ssh_s="${C_DIM}OFF${C_RESET}"
    if pgrep sshd >/dev/null; then
        local conn=$(netstat -tn 2>/dev/null | grep ':8022' | grep 'ESTABLISHED' | wc -l)
        if [ "$conn" -gt 0 ]; then ssh_s="${C_ERR}âš ï¸ $connäºº${C_RESET}"; else ssh_s="${C_OK}ON${C_RESET}"; fi
    fi

    # å®šç‚¹ç»˜åˆ¶ (ä¸æ¸…å±ï¼Œæ— é—ªçƒ)
    draw_at 1 15 "${C_HL}XXY SYSTEM ${APP_VER}${C_RESET}"
    
    draw_at 3 3 "â° æ—¶é—´: ${C_TEXT}${time}${C_RESET}   ğŸ¨ ä¸»é¢˜: ${C_TITLE}${THEME_NAME}${C_RESET}   "
    draw_at 4 3 "ğŸ“¡ ç½‘ç»œ: ${C_TEXT}${ip}${C_RESET}        " # ç©ºæ ¼æ˜¯ä¸ºäº†è¦†ç›–æ—§é•¿å­—ç¬¦
    draw_at 5 3 "ğŸ’¾ å­˜å‚¨: ${C_TEXT}${disk}${C_RESET}       ğŸ§  å†…å­˜: ${C_TEXT}${ram}${C_RESET}   "
    draw_at 6 3 "ğŸ›¡ï¸ SSH : ${ssh_s}           "
    
    # æ¢å¤å…‰æ ‡åˆ°è¾“å…¥ä½ç½®
    tput cup 15 10
}

# ==============================================================================
# ğŸ§  å®¹å™¨å·¥å•ç³»ç»Ÿ (Worker System)
# ==============================================================================
create_worker() {
    rm -f "$TMP_DIR/worker.sh"
    cat >> "$TMP_DIR/worker.sh" << 'EOF'
#!/bin/sh
set -e
export DEBIAN_FRONTEND=noninteractive
act=$1

# è‡ªåŠ¨è¯†åˆ«
if [ -f /etc/alpine-release ]; then T="alpine"; PM="apk"
elif [ -f /etc/arch-release ]; then T="arch"; PM="pacman"
elif [ -f /etc/debian_version ]; then T="debian"; PM="apt"
else T="unknown"; fi

case "$act" in
  # --- åŸºç¡€ ---
  src) 
    if [ "$T" = "debian" ]; then 
      if ! grep -q "tsinghua" /etc/apt/sources.list; then
        sed -i 's/http:\/\/ports.ubuntu.com/http:\/\/mirrors.tuna.tsinghua.edu.cn/g' /etc/apt/sources.list
        apt update -y
      fi
    elif [ "$T" = "alpine" ]; then
      sed -i 's/dl-cdn.alpinelinux.org/mirrors.ustc.edu.cn/g' /etc/apk/repositories; apk update
    fi ;;
    
  gui)
    if [ "$T" = "debian" ]; then apt install -y xfce4 xfce4-goodies dbus-x11 tigervnc-standalone-server
    elif [ "$T" = "alpine" ]; then apk add xfce4 dbus-x11 tigervnc; fi
    mkdir -p ~/.vnc; echo "123456" | vncpasswd -f > ~/.vnc/passwd; chmod 600 ~/.vnc/passwd ;;
    
  # --- å¼€å‘å †æ ˆ ---
  stack_web)
    if [ "$T" = "debian" ]; then apt install -y nodejs npm nginx git curl; fi ;;
  stack_py)
    if [ "$T" = "debian" ]; then apt install -y python3 python3-pip python3-venv build-essential; fi ;;
  stack_hack)
    if [ "$T" = "debian" ]; then apt install -y nmap sqlmap hydra netcat-openbsd; fi ;;
    
  # --- åŒ»ç”Ÿ ---
  doctor)
    # ä¿®å¤ DNS
    echo "nameserver 223.5.5.5" > /etc/resolv.conf
    # ä¿®å¤ apt é”
    rm -f /var/lib/dpkg/lock*
    # ä¿®å¤ tmp æƒé™
    chmod 777 /tmp ;;
    
  clean)
    sed -i '/fastfetch/d' /etc/profile /etc/bash.bashrc ~/.bashrc 2>/dev/null ;;
    
  fetch)
    command -v fastfetch >/dev/null || $PM install -y fastfetch || $PM install -y neofetch ;;
esac
EOF
    chmod 777 "$TMP_DIR/worker.sh"
}

run_task() {
    local sys=$1; local desc=$2; local cmd=$3
    # ç»˜åˆ¶ä¸€ä¸ªä¸´æ—¶çš„æµ®å±‚
    draw_at 15 1 "                                             "
    draw_at 15 1 "${C_HL}â³ æ­£åœ¨æ‰§è¡Œ: ${desc}...${C_RESET}"
    
    if proot-distro login "$sys" --shared-tmp -- /bin/sh "$TMP_DIR/worker.sh" "$cmd" >> "$LOG_FILE" 2>&1; then
         draw_at 15 1 "${C_OK}âœ… ${desc} å®Œæˆ            ${C_RESET}"
    else
         draw_at 15 1 "${C_ERR}âš ï¸ ${desc} å¤±è´¥ (è§æ—¥å¿—)   ${C_RESET}"
    fi
    sleep 1
    # æ¸…é™¤æç¤º
    draw_at 15 1 "â¤ æŒ‡ä»¤:                         "
}

# ==============================================================================
# ğŸ® ä¸šåŠ¡åŠŸèƒ½æ¨¡å—
# ==============================================================================

get_systems() { if [ -d "$ROOTFS" ]; then ls -1 "$ROOTFS" 2>/dev/null; fi; }

# æ™ºèƒ½é€‰æ‹©å™¨
select_sys() {
    tput cnorm; clear
    echo -e "${C_TITLE}=== ç³»ç»Ÿé€‰æ‹©å™¨ ===${C_RESET}"
    mapfile -t LIST < <(get_systems)
    if [ ${#LIST[@]} -eq 0 ]; then echo -e "${C_ERR}æ— ç³»ç»Ÿï¼Œè¯·å…ˆå®‰è£…ï¼${C_RESET}"; read -n 1; return 1; fi
    
    for i in "${!LIST[@]}"; do echo -e "${C_HL}$((i+1)).${C_RESET} ${LIST[$i]}"; done
    echo "0. è¿”å›"
    
    read -p "åºå·: " idx
    if [[ "$idx" =~ ^[0-9]+$ ]] && [ "$idx" -gt 0 ] && [ "$idx" -le "${#LIST[@]}" ]; then
        SELECTED="${LIST[$((idx-1))]}"
        return 0
    elif [ "$idx" = "0" ]; then return 2
    else return 1; fi
}

# [1] ç™»å½•
mod_login() {
    select_sys; [ $? -ne 0 ] && return
    create_worker
    run_task "$SELECTED" "ç¯å¢ƒå‡€åŒ–" "clean"
    tput cnorm; clear
    proot-distro login "$SELECTED" --shared-tmp -- /bin/bash -c "if command -v fastfetch >/dev/null; then fastfetch; fi; exec bash"
    # é€€å‡ºåé‡ç»˜ç•Œé¢
    draw_frame_static
}

# [2] å®‰è£…
mod_install() {
    tput cnorm; clear
    echo -e "${C_TITLE}=== å®‰è£…å‘å¯¼ ===${C_RESET}"
    if ! ping -c 1 -W 1 223.5.5.5 >/dev/null 2>&1; then echo -e "${C_ERR}æ— ç½‘ç»œï¼${C_RESET}"; read -n 1; return; fi
    
    echo -e "1. Ubuntu\n2. Debian\n3. ArchLinux\n4. Alpine\n0. è¿”å›"
    read -p "é€‰æ‹©: " idx
    [ "$idx" = "0" ] && return
    
    local map=("skip" "ubuntu" "debian" "archlinux" "alpine")
    local target=${map[$idx]}
    if [ -z "$target" ]; then return; fi
    
    echo -e "${C_HL}æ­£åœ¨ä¸‹è½½...${C_RESET}"
    proot-distro reset "$target" >/dev/null 2>&1
    rm -rf "$PREFIX/var/lib/proot-distro/download"
    
    if proot-distro install "$target"; then
        create_worker
        run_task "$target" "æ¢æº" "src"
        run_task "$target" "ç¾åŒ–" "fetch"
        echo -e "${C_OK}å®‰è£…æˆåŠŸï¼${C_RESET}"; read -n 1
    else
        echo -e "${C_ERR}å®‰è£…å¤±è´¥ã€‚${C_RESET}"; read -n 1
    fi
    draw_frame_static
}

# [3] æ¡Œé¢
mod_gui() {
    select_sys; [ $? -ne 0 ] && return
    create_worker
    run_task "$SELECTED" "æ£€æŸ¥æ¡Œé¢" "gui"
    
    clear; echo -e "${C_TITLE}å¯åŠ¨æ¨¡å¼${C_RESET}"
    echo "1. æœ¬åœ° (Termux:X11)"
    echo "2. è¿œç¨‹ (VNC)"
    read -p "é€‰æ‹©: " m
    
    if [ "$m" == "1" ]; then
        pkill -9 termux-x11 >/dev/null 2>&1
        rm -rf $PREFIX/tmp/.X11-unix
        am start --user 0 -n com.termux.x11/com.termux.x11.MainActivity >/dev/null 2>&1
        sleep 1
        pulseaudio --start --load="module-native-protocol-tcp auth-ip-acl=127.0.0.1 auth-anonymous=1" --exit-idle-time=-1; termux-x11 :0 &
        proot-distro login "$SELECTED" --shared-tmp -- bash -c "export DISPLAY=:0 PULSE_SERVER=127.0.0.1; dbus-launch --exit-with-session startxfce4"
    elif [ "$m" == "2" ]; then
        proot-distro login "$SELECTED" -- bash -c "vncserver -kill :1 >/dev/null 2>&1; rm -rf /tmp/.X11-unix/X1; vncserver :1 -geometry 1280x720"
        echo -e "åœ°å€: 127.0.0.1:5901  å¯†ç : 123456"; read -n 1
    fi
    draw_frame_static
}

# [4] é¹°çœ¼
mod_ssh() {
    while true; do
        tput cnorm; clear
        echo -e "${C_TITLE}=== é¹°çœ¼é›·è¾¾ ===${C_RESET}"
        USER=$(whoami); IP=$(ifconfig 2>/dev/null | grep -oE 'inet (addr:)?([0-9]*\.){3}[0-9]*' | grep -v '127.0.0.1' | head -1 | awk '{print $2}' | sed 's/addr://')
        
        echo -e "IP: ${C_TEXT}$IP${C_RESET}"
        netstat -tn 2>/dev/null | grep ':8022' | grep 'ESTABLISHED'
        
        echo -e "\n1. å¯åŠ¨\n2. åœæ­¢\n3. è¸¢äºº\n0. è¿”å›"
        read -p "æŒ‡ä»¤: " c
        case "$c" in
            0) break ;;
            1) grep -q "^$USER:" "$PREFIX/etc/shadow" 2>/dev/null || passwd; pkill sshd; sshd ;;
            2) pkill sshd ;;
            3) pgrep sshd | grep -v $(pgrep -o sshd) | xargs kill -9 2>/dev/null ;;
        esac
    done
    draw_frame_static
}

# [5] å¼€å‘è€…å †æ ˆ (æ–°å¢)
mod_dev() {
    select_sys; [ $? -ne 0 ] && return
    create_worker
    
    clear; echo -e "${C_TITLE}=== å¼€å‘è€…å †æ ˆä¸€é”®éƒ¨ç½² ===${C_RESET}"
    echo -e "1. Web å…¨æ ˆ (NodeJS + Nginx + Git)"
    echo -e "2. Python æ•°æ®æ ˆ (Python3 + Pip + Venv)"
    echo -e "3. é»‘å®¢å·¥å…·ç®± (Nmap + SQLMap + Hydra)"
    echo -e "0. è¿”å›"
    
    read -p "é€‰æ‹©å®‰è£…åŒ…: " s
    case "$s" in
        1) run_task "$SELECTED" "Webç¯å¢ƒ" "stack_web" ;;
        2) run_task "$SELECTED" "Pythonç¯å¢ƒ" "stack_py" ;;
        3) run_task "$SELECTED" "é»‘å®¢å·¥å…·" "stack_hack" ;;
    esac
    read -n 1 -s -p "æŒ‰é”®è¿”å›..."
    draw_frame_static
}

# [6] ç³»ç»ŸåŒ»ç”Ÿ (æ–°å¢)
mod_doctor() {
    select_sys; [ $? -ne 0 ] && return
    create_worker
    
    echo -e "\n${C_HL}æ­£åœ¨è¿›è¡Œç³»ç»Ÿè¯Šæ–­ä¸ä¿®å¤...${C_RESET}"
    run_task "$SELECTED" "ä¿®å¤DNS/æ–‡ä»¶é”/æƒé™" "doctor"
    
    echo -e "${C_OK}ä¿®å¤å®Œæˆã€‚${C_RESET}"; read -n 1
    draw_frame_static
}

# [7] ä¸»é¢˜åˆ‡æ¢
mod_theme() {
    clear; echo -e "${C_TITLE}=== ä¸»é¢˜åˆ‡æ¢ ===${C_RESET}"
    echo -e "1. èµ›åšæœ‹å…‹ (Cyberpunk)"
    echo -e "2. é»‘å®¢å¸å›½ (Matrix)"
    echo -e "3. æ·±æµ·å¹½è“ (DeepSea)"
    read -p "é€‰æ‹©: " t
    case "$t" in
        1) set_theme_cyber ;;
        2) set_theme_hacker ;;
        3) set_theme_ocean ;;
    esac
    draw_frame_static
}

# [9] è¯´æ˜ä¹¦
mod_help() {
    clear; echo -e "${C_TITLE}ä½¿ç”¨å¸®åŠ©${C_RESET}"
    echo -e "1. å¿…é¡»æŒ‚ VPN ä¸‹è½½"; echo -e "2. ç”µè„‘è¿SSHç”¨ CMD: ssh -p 8022 user@ip"
    read -n 1; draw_frame_static
}

# --- Main Loop ---
if ! command -v proot-distro >/dev/null; then pkg install proot-distro -y >/dev/null 2>&1; fi

# é¦–æ¬¡ç»˜åˆ¶
draw_frame_static

while true; do
    # ä»…åˆ·æ–°æ•°æ®åŒº
    refresh_hud
    
    # éé˜»å¡è¾“å…¥
    read -t 1 -n 1 key
    if [ -n "$key" ]; then
        case "$key" in
            1) mod_login ;; 2) mod_install ;; 3) mod_gui ;; 4) mod_ssh ;;
            5) mod_dev ;; 6) mod_doctor ;; 7) mod_theme ;; 9) mod_help ;;
            0) exit 0 ;;
        esac
    fi
done
MAIN_EOF

# --- å¯åŠ¨ ---
chmod 755 $PREFIX/bin/xxy
clear
echo -e "\033[1;35mğŸŒŒ å¥‡ç‚¹Â·é™ä¸´ v4000.0 å·²å°±ç»ª\033[0m"
echo -e "-------------------------------------------"
echo -e "ğŸ¨ [UIå†…æ ¸] å·®å¼‚åŒ–æ¸²æŸ“å¼•æ“ï¼Œå½»åº•æœç»é—ªçƒ"
echo -e "ğŸ› ï¸ [å¼€å‘è€…] æ”¯æŒä¸€é”®éƒ¨ç½² Web/Python/Hacker ç¯å¢ƒ"
echo -e "ğŸ’‰ [ç³»ç»ŸåŒ»ç”Ÿ] ä¸€é”®ä¿®å¤å®¹å™¨å†…å„ç±»ç–‘éš¾æ‚ç—‡"
echo -e "ğŸ­ [ä¸»é¢˜åº“] å†…ç½® 3 å¥—é«˜é¢œå€¼é…è‰²æ–¹æ¡ˆ"
echo -e "-------------------------------------------"
read -n 1 -p "æŒ‰ä»»æ„é”®å¯åŠ¨..."
xxy
